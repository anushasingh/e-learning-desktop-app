<AnchorPane fx:id="side_pane" prefHeight="650.0" prefWidth="357.0" style="-fx-background-color: #90CAF9;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignUpController">
   <children>
      <ImageView fitHeight="47.0" fitWidth="133.0" layoutX="92.0" layoutY="194.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Images/logotransparent.png" />
         </image>
      </ImageView>
      <TextField fx:id="user" layoutX="60.0" layoutY="296.0" prefHeight="29.0" prefWidth="197.0" promptText="Username">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <PasswordField fx:id="pass" layoutX="60.0" layoutY="347.0" prefHeight="29.0" prefWidth="197.0" promptText="Password">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </PasswordField>
      <JFXButton fx:id="signup" layoutX="114.0" layoutY="387.0" onAction="#signUp" text="SIGN UP" textFill="#1a195e">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </JFXButton>
      <Label fx:id="res" layoutX="71.0" layoutY="420.0" prefHeight="17.0" prefWidth="237.0" textFill="#ee0404">
